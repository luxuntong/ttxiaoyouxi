<root>
    <Properties>
        <sessionKey>
            <Type>        UNICODE        </Type>
            <Flags>         BASE        </Flags>
        </sessionKey>

        <isInit>
            <Type>        BOOL                   </Type>
            <Flags>       BASE                   </Flags>
            <Default>     0                      </Default>
            <Persistent>  true                   </Persistent>
        </isInit>

        <gbID>
            <Type>        GBID                   </Type>
            <Flags>       BASE                   </Flags>
            <Persistent>  true                   </Persistent>
        </gbID>

        <gbIdc>
            <Type>        GBID                   </Type>
            <Flags>       CELL_PRIVATE           </Flags>
            <Persistent>  true                   </Persistent>
        </gbIdc>

        <openId>
            <Type>        UNICODE                </Type>
            <Flags>         BASE                </Flags>
        </openId>

        <modelID>
            <Type>        UINT8            </Type>
            <Flags>        ALL_CLIENTS            </Flags>
            <Default>        0                </Default>
        </modelID>

        <accountName>
            <Type>        UNICODE            </Type>
            <Flags>        ALL_CLIENTS            </Flags>
        </accountName>

        <avatarName>
            <Type>        UNICODE            </Type>
            <Flags>        ALL_CLIENTS            </Flags>
        </avatarName>

        <dbid>
            <Type>        DBID                </Type>
            <Flags>        CELL_PUBLIC            </Flags>
            <Default>        0                </Default>
        </dbid>

        <sessionId>
            <Type>        STRING                </Type>
            <Flags>         BASE                </Flags>
        </sessionId>

        <decryptedData>
            <Type>        PY_DICT                </Type>
            <Flags>         BASE                </Flags>
        </decryptedData>

        <seed>
            <Type>          UINT16              </Type>
            <Flags>         ALL_CLIENTS         </Flags>
        </seed>

        <curIndex>
            <Type>          INT16              </Type>
            <Flags>         CELL_PUBLIC         </Flags>
            <Default>       0                   </Default>
        </curIndex>

        <curPos>
            <Type>          PYTHON              </Type>
            <Flags>         CELL_PUBLIC         </Flags>
        </curPos>

        <HP>
            <Type>          INT8                </Type>
            <Flags>         ALL_CLIENTS         </Flags>
        </HP>

        <items>
            <Type>          PYTHON              </Type>
            <Flags>         CELL_PRIVATE        </Flags>
        </items>
    </Properties>

    <BaseMethods>
        <createCell>
            <Arg>    ENTITYCALL </Arg>
        </createCell>

        <matchCoop> <Exposed/>
        </matchCoop>

        <decodeEncryptedData>
            <Exposed/>
            <Arg>    UNICODE     </Arg>
            <Arg>    UNICODE     </Arg>
        </decodeEncryptedData>

        <onLeaveRoom>
        </onLeaveRoom>
    </BaseMethods>

    <CellMethods>
        <jump> <Exposed/>
            <Arg>   FLAT_INDEX                  </Arg>
            <Arg>   ARRAY <of>   FLOAT   </of>         </Arg>
            <Arg>   INT32                   </Arg>
        </jump>

        <leaveRoom> <Exposed/>
        </leaveRoom>

        <getItem> <Exposed/>
            <Arg>   FLAT_INDEX                  </Arg>
        </getItem>

        <useItem> <Exposed/>
            <Arg>   UINT8                       </Arg>
            <Arg>   ENTITY_ID                    </Arg>
        </useItem>
    </CellMethods>

    <ClientMethods>
        <onDie>
            <Arg>        ENTITY_ID    </Arg>
        </onDie>

        <onStartWalk>
            <Arg>        INT32    </Arg>
        </onStartWalk>

        <onStopWalk>
            <Arg>        VECTOR3            </Arg>
        </onStopWalk>

        <onLeftJump>
        </onLeftJump>

        <onRightJump>
        </onRightJump>

        <onPickUpItem>
                <Arg>        ENTITY_ID        </Arg>
                <Arg>        VECTOR3            </Arg>
        </onPickUpItem>

        <onThrowItem>
                <Arg>        ENTITY_ID        </Arg>
                <Arg>        VECTOR3            </Arg>
        </onThrowItem>

        <onNewTurn>
                <Arg>        ENTITY_ID        </Arg>
                <Arg>        INT32            </Arg>
        </onNewTurn>

        <onRecoverItem>
            <Arg>        ENTITY_ID    </Arg>
        </onRecoverItem>

        <onRecvDamage>
            <Arg>        ENTITY_ID    </Arg>
            <Arg>        INT32        </Arg>
            <Arg>        INT32        </Arg>
        </onRecvDamage>

        <onGameOver>
            <Arg>        BOOL    </Arg>
            <Arg>        FLOAT    </Arg>
            <Arg>        INT32        </Arg>
            <Arg>        INT32        </Arg>
            <Arg>        INT32        </Arg>
        </onGameOver>

        <onResetItem>
            <Arg>        ENTITY_ID        </Arg>
            <Arg>        VECTOR3            </Arg>
        </onResetItem>

        <onContinueGame>
            <Arg>        ENTITY_ID        </Arg>
        </onContinueGame>

        <onJump>
            <Arg>    UINT16         </Arg>
            <Arg>   ARRAY <of>   FLOAT   </of>         </Arg>
            <Arg>   INT32                   </Arg>
        </onJump>

        <onJumpResult>
            <Arg>   BOOL            </Arg>
        </onJumpResult>

        <onGetItem>
            <Arg>   ENTITY_ID       </Arg>
            <Arg>   FLAT_INDEX      </Arg>
            <Arg>   INT8            </Arg>
        </onGetItem>

        <onReset>
        </onReset>

        <onGetRelivePos>
            <Arg>   ENTITY_ID       </Arg>
            <Arg>   ARRAY <of>   FLOAT   </of>         </Arg>
        </onGetRelivePos>

        <onJumpCompleted>
            <Arg>   ENTITY_ID       </Arg>
        </onJumpCompleted>

        <onUseItemRet>
            <Arg>   ENTITY_ID       </Arg>
            <Arg>   UINT8           </Arg>
            <Arg>   UINT8           </Arg>
            <Arg>   FLAT_INDEX      </Arg>
        </onUseItemRet>
    </ClientMethods>

</root>
