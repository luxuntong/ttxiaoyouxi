<root>
    <Properties>
        <sessionKey>
            <Type>        UNICODE        </Type>
            <Flags>         BASE        </Flags>
        </sessionKey>

        <isInit>
            <Type>        BOOL                   </Type>
            <Flags>       BASE                   </Flags>
            <Default>     0                      </Default>
            <Persistent>  true                   </Persistent>
        </isInit>

        <gbID>
            <Type>        GBID                   </Type>
            <Flags>       BASE                   </Flags>
            <Persistent>  true                   </Persistent>
        </gbID>

        <openId>
            <Type>        UNICODE                </Type>
            <Flags>         BASE                </Flags>
        </openId>

        <modelID>
            <Type>        UINT8            </Type>
            <Flags>        ALL_CLIENTS            </Flags>
            <Default>        0                </Default>
        </modelID>

        <roomKey>
            <Type>        SPACE_KEY            </Type>
            <Flags>        BASE                </Flags>
            <Persistent>        true                </Persistent>
        </roomKey>

        <accountName>
            <Type>        UNICODE            </Type>
            <Flags>        ALL_CLIENTS            </Flags>
        </accountName>

        <avatarName>
            <Type>        UNICODE            </Type>
            <Flags>        ALL_CLIENTS            </Flags>
        </avatarName>

        <dbid>
            <Type>        DBID                </Type>
            <Flags>        CELL_PUBLIC            </Flags>
            <Default>        0                </Default>
        </dbid>

        <HP>
            <Type>        INT32                </Type>
            <Flags>        ALL_CLIENTS            </Flags>
            <Default>        100                </Default>
        </HP>

        <hitCount>
            <Type>        INT32                </Type>
            <Flags>      CELL_PUBLIC_AND_OWN    </Flags>
            <Default>        0                </Default>
        </hitCount>

        <throwCount>
            <Type>        INT32                </Type>
            <Flags>     CELL_PUBLIC_AND_OWN    </Flags>
            <Default>        0                </Default>
        </throwCount>

        <totalHarm>
            <Type>        INT32                </Type>
            <Flags>      CELL_PUBLIC_AND_OWN    </Flags>
            <Default>        0                </Default>
        </totalHarm>

        <totalTime>
            <Type>        INT32                </Type>
            <Flags>      CELL_PUBLIC_AND_OWN    </Flags>
            <Default>        0                </Default>
        </totalTime>

        <score>
            <Type>        INT32                </Type>
            <Flags>     CELL_PUBLIC_AND_OWN    </Flags>
            <Default>        0                </Default>
        </score>

        <hitRate>
            <Type>        FLOAT                </Type>
            <Flags>     CELL_PUBLIC_AND_OWN    </Flags>
            <Default>        0                </Default>
        </hitRate>

        <sessionId>
            <Type>        STRING                </Type>
            <Flags>         BASE                </Flags>
        </sessionId>

        <decryptedData>
            <Type>        PY_DICT                </Type>
            <Flags>         BASE                </Flags>
        </decryptedData>
    </Properties>

    <ClientMethods>
        <onDie>
            <Arg>        ENTITY_ID    </Arg>
        </onDie>

        <onStartWalk>
            <Arg>        INT32    </Arg>
        </onStartWalk>

        <onStopWalk>
            <Arg>        VECTOR3            </Arg>
        </onStopWalk>

        <onLeftJump>
        </onLeftJump>

        <onRightJump>
        </onRightJump>

        <onPickUpItem>
                <Arg>        ENTITY_ID        </Arg>
                <Arg>        VECTOR3            </Arg>
        </onPickUpItem>

        <onThrowItem>
                <Arg>        ENTITY_ID        </Arg>
                <Arg>        VECTOR3            </Arg>
        </onThrowItem>

        <onNewTurn>
                <Arg>        ENTITY_ID        </Arg>
                <Arg>        INT32            </Arg>
        </onNewTurn>

        <onRecoverItem>
            <Arg>        ENTITY_ID    </Arg>
        </onRecoverItem>

        <onRecvDamage>
            <Arg>        ENTITY_ID    </Arg>
            <Arg>        INT32        </Arg>
            <Arg>        INT32        </Arg>
        </onRecvDamage>

        <onGameOver>
            <Arg>        BOOL    </Arg>
            <Arg>        FLOAT    </Arg>
            <Arg>        INT32        </Arg>
            <Arg>        INT32        </Arg>
            <Arg>        INT32        </Arg>
        </onGameOver>

        <onResetItem>
            <Arg>        ENTITY_ID        </Arg>
            <Arg>        VECTOR3            </Arg>
        </onResetItem>

        <onContinueGame>
            <Arg>        ENTITY_ID        </Arg>
        </onContinueGame>

        <onJump>
        </onJump>
    </ClientMethods>

    <BaseMethods>
        <createCell>
            <Arg>    ENTITYCALL </Arg>
        </createCell>

        <joinRoom>
            <Exposed/>
        </joinRoom>

        <decodeEncryptedData>
            <Exposed/>
            <Arg>    UNICODE     </Arg>
            <Arg>    UNICODE     </Arg>
        </decodeEncryptedData>
    </BaseMethods>

    <CellMethods>
        <startWalk>
            <Exposed/>
            <Arg>        INT32    </Arg>
        </startWalk>

        <stopWalk>
            <Exposed/>
            <Arg>        VECTOR3        </Arg>
        </stopWalk>

        <rightJump>
            <Exposed/>
        </rightJump>

        <leftJump>
            <Exposed/>
        </leftJump>

        <pickUpItem>
            <Exposed/>
            <Arg>        ENTITY_ID        </Arg>
            <Arg>        VECTOR3        </Arg>
        </pickUpItem>

        <throwItem>
            <Exposed/>
            <Arg>        ENTITY_ID    </Arg>
            <Arg>        VECTOR3        </Arg>
        </throwItem>

        <newTurn>
            <Exposed/>
        </newTurn>

        <jump>
            <Exposed/>
        </jump>

        <recoverItem>
            <Exposed/>
            <Arg>        ENTITY_ID    </Arg>
        </recoverItem>

        <recvDamage>
            <Exposed/>
            <Arg>        ENTITY_ID    </Arg>
        </recvDamage>

        <resetItem>
            <Exposed/>
            <Arg>        ENTITY_ID    </Arg>
        </resetItem>

        <addItem>
            <Exposed/>
            <Arg>        INT32    </Arg>
        </addItem>

        <continueGame>
            <Exposed/>
        </continueGame>

    </CellMethods>

</root>
